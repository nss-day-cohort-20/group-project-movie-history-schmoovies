{{#each movies}}
	<div class="unwatchedCard" id="movie{{id}}">
		<div class="cardHeader">
			<h2>{{title}}</h2>
			<button class="deleteCardBtn">X</button>
		</div>
		<div class="movieInfo">
			{{ > movieInfoPartial }}
			{{!-- movie info partial goes here --}}
		</div>
		<div id="rateYo">{{$("#rateYo").rateYo({
    rating: 3.6
  })}}</div>
	</div>
{{/each}}